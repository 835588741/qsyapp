<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--  gradle升级到5.6.4后 更要求复用原则，编译时不允许出现一个属性多次重复出现使用，所以这里需要把同名复用的提取出来 start -->
    <attr name="imagewidth" format="dimension"/>
    <attr name="imageheight" format="dimension"/>
    <attr name="textColor" format="color"/>
    <attr name="textSize" format="dimension"/>
    <!--  gradle升级到5.6.4后 更要求复用原则，编译时不允许出现一个属性多次重复出现使用，所以这里需要把同名复用的提取出来 end -->

    <declare-styleable name="downloadProgressBar">
        <attr name="dptextsize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomSeekBar">
        <attr name="csb_progress_height" format="dimension" />
        <attr name="csb_progress_corner" format="dimension" />
        <attr name="csb_section_radius" format="dimension" />
        <attr name="csb_section_enable" format="boolean" />
        <attr name="csb_thumb_radius" format="dimension" />
        <attr name="csb_thumb_point" format="reference"/>
        <attr name="csb_background_color" format="color" />
        <attr name="csb_progress_color" format="color" />
        <attr name="csb_min_value" format="integer" />
        <attr name="csb_max_value" format="integer" />
        <attr name="csb_overspread_enable" format="boolean" />
        <attr name="csb_thumb_scale_enable" format="boolean" />
        <attr name="csb_thumb_anim_enable" format="boolean" />

        <attr name="csb_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="VideoSeekBar">
        <attr name="imagewidth"/>
        <attr name="imageheight"/>
    </declare-styleable>

    <declare-styleable name="TimeCutBar">
        <attr name="tcb_time" format="integer"/>
        <attr name="tcb_progress_color" format="color"/>
        <attr name="tcb_progress_height" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="DoubleSlideSeekBar">
        <attr name="lineHeight" format="dimension"/>
        <attr name="textSize"/>
        <attr name="textColor"/>
        <attr name="inColor" format="color"/>
        <attr name="outColor" format="color"/>
        <attr name="imageLow" format="reference"/>
        <attr name="imageBig" format="reference"/>
        <attr name="imagewidth"/>
        <attr name="imageheight"/>
        <attr name="hasRule" format="boolean"/>
        <attr name="ruleColor" format="color"/>
        <attr name="ruleTextColor" format="color"/>
        <attr name="unit" format="string"/>
        <attr name="equal" format="integer"/>
        <attr name="ruleUnit" format="string"/>
        <attr name="ruleTextSize" format="dimension"/>
        <attr name="ruleLineHeight" format="dimension"/>
        <attr name="bigValue" format="integer"/>
        <attr name="smallValue" format="integer"/>
    </declare-styleable>

    <declare-styleable name="RatioImageView">
        <attr name="ratio" format="float"/>
    </declare-styleable>

    <declare-styleable name="ClipViewLayout">
        <attr name="clipType" format="enum">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
            <enum name="palace" value="3" />
        </attr>
        <attr name="horizontalPadding" format="dimension" />
        <attr name="clipBorderWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SelectView">
        <attr name="isCanvasLine" format="boolean"/>
        <attr name="svTextSize" format="dimension"/>
        <attr name="unitSize" format="dimension"/>
        <attr name="bgColor" format="color"/>
        <attr name="textColor"/>
        <attr name="textSelectColor" format="color"/>
        <attr name="unitLongLine" format="integer"/>
    </declare-styleable>

    <declare-styleable name="StrokeText">
        <attr name="strokeColor" format="color"/>
        <attr name="openStroke" format="boolean"/>
    </declare-styleable>
</resources>